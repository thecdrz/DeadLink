{
  "name": "deadlink",
  "version": "2.12.3",
  "description": "Advanced Discord bridge bot for 7 Days to Die with interactive dashboard, dynamic narrative storytelling, and intelligent activity tracking. Built on Dishorde by LakeYS with comprehensive enhancements by CDRZ.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest --runInBand",
  "test:json": "node -e \"require('fs').mkdirSync('logs',{recursive:true})\" && jest --runInBand --json --outputFile logs/jest-last-run.json",
  "test:coverage": "node -e \"require('fs').mkdirSync('logs',{recursive:true})\" && jest --runInBand --coverage --coverageReporters=json-summary --json --outputFile logs/jest-last-run.json",
    "slash:global": "node scripts/register-commands.js --global",
  "slash:guild": "node scripts/register-commands.js --guild",
  "snapshots": "node scripts/snapshot-embeds.js",
  "snapshots:anim": "node scripts/snapshot-anim.js",
  "telemetry:serve": "node scripts/telemetry-server.js",
  "telemetry:send": "node scripts/send-telemetry-once.js",
  "telemetry:flush": "node scripts/telemetry-flush-buffer.js",
  "telemetry:smoke": "node scripts/telemetry-smoke.js",
  "telemetry:report": "node scripts/telemetry-report.js --input .\\logs\\telemetry-events.jsonl --since 30d --out txt",
  "telemetry:report:md": "node scripts/telemetry-report.js --input .\\logs\\telemetry-events.jsonl --since 30d --out md > .\\logs\\telemetry-report.md",
  "telemetry:report:html": "node scripts/telemetry-report.js --input .\\logs\\telemetry-events.jsonl --since 90d --out html --output .\\logs\\telemetry-report.html",
  "site:telemetry": "node scripts/telemetry-report.js --input .\\logs\\telemetry-events.jsonl --since 90d --out html --output .\\docs\\telemetry-report.html"
  },
  "author": "CDRZ (built on Dishorde by LakeYS)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thecdrz/DeadLink"
  },
  "keywords": [
    "discord",
    "7-days-to-die",
    "bot",
    "telnet",
    "gaming",
    "server-management",
    "analytics",
    "dashboard"
  ],
  "dependencies": {
    "discord.js": "^13.17.1",
    "dotenv": "^16.6.1",
    "minimist": "^1.2.5",
    "semver-compare": "^1.0.0",
    "telnet-client": "^0.15.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
  "jest": "^29.7.0",
  "puppeteer": "^24.9.0",
  "ffmpeg-static": "^5.2.0"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.3.0",
  "zod": "^3.25.76",
  "pino": "^9.2.0",
  "pino-pretty": "^11.2.2"
  }
}
