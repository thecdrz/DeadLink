{
  "token": "YOUR_DISCORD_BOT_TOKEN",
  "channel": "YOUR_CHANNEL_ID",
  "ip": "127.0.0.1",
  "port": 8081,
  "password": "YOUR_TELNET_PASSWORD",
  "dev-mode": false,

  "allow-exec-command": false,
  "allow-links-from-game": false,
  "allow-multiple-instances": true,
  "disable-commands": false,

  "log-console": false,
  "log-messages": false,
  "log-telnet": true,

  "show-private-chat": false,
  "show-discord-bot-msgs": false,
  "disable-status-updates": false,
  "disable-version-check": false,
  "hide-prefix": false,

  "disable-chatmsgs": false,
  "disable-join-leave-gmsgs": false,
  "disable-misc-gmsgs": false,
  "disable-non-player-chatmsgs": true,

  "horde-frequency": 7,

  "analytics": {
    "enabled": true,
    "endpoint": "https://telemetry.deadlink.lol/v1/event",
    "storagePath": "./telemetry.json"
  },

  "updates": {
    "enabled": true,
    "intervalHours": 24,
    "prerelease": false,
    "notifyMode": "off",
    "notifyChannel": ""
  },
  "bloodMoon": {
    "enabled": true,
    "intervalSeconds": 60,
    "frequency": 7,
    "broadcastInGame": true
  }
}
